{% extends 'web/web-base.html.twig' %}

{% block body %}

<div class="generic-content">
	<div class="container-fluid content-container">
		<div class="container">
            <div class="row">
                <div class="title">
                    {{ 'tid_equip_permanent_title'|trans|upper }}
                </div>
            </div>
            <div class="row">
                <div class="col-lg-4 content-image">
                    <img id='equip-permanent' src='/images/entitat/equip-permanent.jpg' onClick=clickedThumbImage('equip-permanent'); >
