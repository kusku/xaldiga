{% extends 'intranet/base.html.twig' %}

{% block content %}
<div class="breadcrumb-holder">
  <div class="container-fluid">
    <ul class="breadcrumb">
      <li class="breadcrumb-item"><a href="/intranet/calendar">Calendari</a></li>
      <li class="breadcrumb-item active">Crear acte</li>
    </ul>
  </div>
</div>
<section>
  <div class="container-fluid">
    <header> 
      <h1 class="display">Calendari</h1>
    </header>

    <div class="line"></div>
    <div class="form-group row">
      <div class="col-lg-12">
        <div class="card">
          <div class="card-header">
            <h4>Crear Acte</h4>
          </div>
          <div class="card-body">
            {{ form_start(form) }}
            {{ form_widget(form) }}
            <input type="submit" value={{ 'Crear'|trans }} class="btn btn-primary" />
            {{ form_end(form) }}
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
{% endblock %}

